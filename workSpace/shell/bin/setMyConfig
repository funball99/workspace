#!/bin/bash

## function

function my(){
PS1="(╯°Д °)╯︵(不努力的\u@\W)\$ "
}
function you(){
PS1="[\u@\W]\$ "
}
function ..(){
cd ..
}
function vimbash(){
vim ~/.bash_profile
}
function srcbash(){
source ~/.bash_profile
}
function vimcommon(){
vim /imcoming
}
function srccommon(){
source /imcoming
}
function apacherestart(){
service httpd restart
}
function apacheconfig(){
vim /etc/httpd/conf/httpd.conf	
}
function workspace(){
if [ ! -d /myDisk/wilson/workspace/workSpace ]; then
	mkdir -p /myDisk/wilson/workspace/workSpace
fi
	cd /myDisk/wilson/workspace/workSpace
}
function bin(){
if [ ! -d /myDisk/wilson/workspace/workSpace/shell/bin ]; then
	mkdir -p /myDisk/wilson/workspace/workSpace/shell/bin
fi
	cd /myDisk/wilson/workspace/workSpace/shell/bin
}
function cdls(){
builtin cd $* && ls
}
function wilson(){
cd /myDisk/wilson
}
function webroot(){
cd /myDisk/wilson/workspace/workSpace/apacheDocument
}

## alias 
alias cd='cdls'


