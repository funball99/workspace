#!/bin/bash
source ~/.bash_profile

BIN="/myDisk/wilson/workspace/workSpace/shell/bin"
GITSPACE="/myDisk/wilson/workspace"
if [ /imcoming -nt $BIN/setMyConfig ]; then
	if [ -e $BIN/setMyConfig.bak ]; then
		source	$BIN/bakWorkSpace &&	rm $BIN/setMyConfig.bak && cp $BIN/setMyConfig $BIN/setMyConfig.bak && rm $BIN/setMyConfig && cp /imcoming $BIN/setMyConfig && chmod 755 /imcoming $BIN/setMyConfig && cd $GITSPACE && git add -A && git commit -m "$(date +%Y%m%d%H%M)" && git push git@github.com:funball99/workspace.git && echo "sync succeed at $(date +%Y%m%d%H%M)!" 
		echo "========================================================================="
	fi
else
	cp $BIN/setMyConfig /imcoming
	echo "latest version at $(date +%Y%m%d%H%M) !"
	echo "========================================================================="
fi
